{
  "name": "redux-todomvc",
  "version": "1.0.0",
  "description": "Redux TodoMVC application.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "npm run clean && npm run build:assets",
    "build": "NODE_ENV=production npm run build:js",
    "build:assets": "./scripts/build-html.sh && ./scripts/build-css.sh",
    "build:js": "webpack --colors --progress",
    "clean": "npm run clean:assets && npm run clean:js",
    "clean:assets": "rimraf public/css && mkdir -p public/css",
    "clean:js": "rimraf public/js && mkdir -p public/js",
    "prestart": "NODE_ENV=dev npm run prebuild",
    "start": "NODE_ENV=dev webpack-dev-server --colors --hot --inline",
    "watch": "NODE_ENV=dev npm run prebuild && NODE_ENV=dev webpack --colors --progress --watch",
    "deploy": "npm run deploy:master && npm run deploy:gh-pages",
    "deploy:master": "git push origin master",
    "deploy:gh-pages": "gh-pages --dist public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sylvaindethier/redux-todomvc.git"
  },
  "author": "Sylvain Dethier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sylvaindethier/redux-todomvc/issues"
  },
  "homepage": "https://github.com/sylvaindethier/redux-todomvc#readme",
  "dependencies": {
    "classnames": "^2.2.3",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "redux-actions": "^0.9.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-external-helpers-2": "^6.3.13",
    "babel-plugin-syntax-jsx": "^6.3.13",
    "babel-plugin-transform-react-jsx": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.13.1",
    "gh-pages": "^0.8.0",
    "rimraf": "^2.5.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
